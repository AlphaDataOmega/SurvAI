{
  "name": "@survai/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for SurvAI MVP",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "echo \"Tests handled by root Jest config\" && exit 0"
  },
  "devDependencies": {
    "typescript": "^5.1.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "survai",
    "shared",
    "types",
    "typescript"
  ],
  "author": "SurvAI Team",
  "license": "MIT"
}